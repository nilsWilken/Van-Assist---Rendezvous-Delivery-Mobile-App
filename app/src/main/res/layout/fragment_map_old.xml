<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_map"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.main.MapFragmentOld">

    <com.mapbox.mapboxsdk.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <!--<include layout="@layout/content_map" />-->
    <!-- include bottom sheet -->
    <include layout="@layout/bottom_sheet" />

    <RelativeLayout
        android:id="@+id/button3D"
        app:layout_anchor="@id/mapView"
        app:layout_anchorGravity="top|end"
        android:background="@drawable/level_button_rounded"
        android:layout_width="@dimen/level_button_size"
        android:gravity="center"
        android:elevation="2dp"
        android:layout_margin="16dp"
        android:layout_height="@dimen/level_button_size">
        <TextView
            android:id="@+id/button3DText"
            android:textSize="@dimen/level_button_text_size"
            android:textColor="@color/nearly_black"
            android:layout_width="wrap_content"
            android:text="3D"
            android:layout_height="wrap_content"/>
    </RelativeLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:layout_marginTop="56dp"
        android:id="@+id/levelSwitch"
        app:layout_anchor="@id/mapView"
        app:layout_anchorGravity="top|end"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_alignParentEnd="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
