    <TextView

        android:id="@+id/textview_vanlocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="280dp"
        android:background="@color/logoLightGrey"
        android:padding="8dp"
        android:text="@string/locate_van"
        android:textColor="@color/logoGrey"
        android:visibility="invisible" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_vanlocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="@dimen/fab_margin"
        android:layout_marginBottom="270dp"
        android:background="?android:attr/selectableItemBackground"
        android:backgroundTint="@color/logoRed"
        android:clickable="true"
        android:focusable="true"
        android:visibility="invisible"
        app:elevation="6dp"
        app:maxImageSize="40dp"
        app:pressedTranslationZ="12dp"
        app:srcCompat="@drawable/ic_local_shipping_black_24dp" />

    <TextView

        android:id="@+id/textview_parkinglocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="210dp"
        android:background="@color/logoLightGrey"
        android:padding="8dp"
        android:text="@string/next_parking_location"
        android:textColor="@color/logoGrey"
        android:visibility="invisible" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_parkinglocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="@dimen/fab_margin"
        android:layout_marginBottom="200dp"
        android:background="?android:attr/selectableItemBackground"
        android:backgroundTint="@color/logoRed"
        android:clickable="true"
        android:focusable="true"
        android:visibility="invisible"
        app:elevation="6dp"
        app:maxImageSize="40dp"
        app:pressedTranslationZ="12dp"
        app:srcCompat="@drawable/ic_local_parking_black_24dp" />

    <TextView

        android:id="@+id/textview_deliverylocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="140dp"
        android:background="@color/logoLightGrey"
        android:padding="8dp"
        android:text="@string/locate_delivery_destination"
        android:textColor="@color/logoGrey"
        android:visibility="invisible" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_deliverylocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="@dimen/fab_margin"
        android:layout_marginBottom="130dp"
        android:background="?android:attr/selectableItemBackground"
        android:backgroundTint="@color/logoRed"
        android:clickable="true"
        android:focusable="true"
        android:visibility="invisible"
        app:elevation="6dp"
        app:maxImageSize="40dp"
        app:pressedTranslationZ="12dp"
        app:srcCompat="@drawable/ic_inbox_black_24dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/fab_margin"
        android:background="?android:attr/selectableItemBackground"
        android:backgroundTint="@color/logoRed"
        android:clickable="true"
        android:focusable="true"
        android:visibility="visible"
        app:borderWidth="0dp"
        app:elevation="6dp"
        app:layout_anchor="@id/bottom_sheet"
        app:layout_anchorGravity="top|end"
        app:maxImageSize="48dp"
        app:pressedTranslationZ="12dp"
        app:srcCompat="@drawable/ic_add_white_36dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/goto_launchpad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top|start"
        android:layout_margin="16dp"
        android:backgroundTint="@color/logoRed"
        android:tint="@android:color/white"
        app:borderWidth="0dp"
        app:maxImageSize="36dp"
        app:srcCompat="@drawable/ic_home_black_24dp" />

</androidx.coordinatorlayout.widget.CoordinatorLayout>